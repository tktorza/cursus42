{% for deliveryCart in deliveryCarts %}
	{% if deliveryCart.min and not deliveryCart.max %}
		Minimum {{ deliveryCart.min }}€
	{% elseif deliveryCart.max and not deliveryCart.max %}
		Maximum {{ deliveryCart.max }}€
	{% elseif deliveryCart.min and deliveryCart.max %}
		Entre {{ deliveryCart.min }}€ et {{ deliveryCart.max }}€
	{% endif %}
	{% if not loop.last %}<br>{% endif %}
{% endfor %}